                          <rng:element name="MultimediaDescription">
                            <rng:element name="ImageItems">
       
                              <rng:oneOrMore>
                                <rng:element name="ImageItem">
                                  <rng:element name="ImageFormat">
                                    <rng:element name="URL">
                                      <rng:ref name="def_url_string"/>
