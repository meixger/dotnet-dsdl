                                    <rng:element name="Description">
                                      <rng:ref name="def_nonempty_string"/>
                                      <rng:attribute name="TextFormat">
                                        <rng:choice>
                                          <rng:value>PlainText</rng:value>
                                          <rng:value>HTML</rng:value>
